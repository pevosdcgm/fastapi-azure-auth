    "version": "0.2.0",
    "configurations": [{
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "envFile": "${workspaceFolder}/.env",

        },
        {
            "name": "Python: demo_project",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/demo_project/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/demo_project/.envTIA",
            "env": {
                "PYTHONPATH": "${cwd}",

            },
            "args": [
                "--reload",
                "--api"
            ],
        },
        {
            "name": "Python: FastAPI template",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--reload",
                "--port",
                "8088"
            ],

            "jinja": true,
            "justMyCode": true
        }

    ]
}